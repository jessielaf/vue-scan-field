{
  "username": {
    "validator": {
      "required": true,
      "max": 150
    },
    "attributes": {
      "element": "text_field"
    }
  },
  "isStaff": {
    "validator": {},
    "attributes": {
      "element": "checkbox"
    }
  },
  "isActive": {
    "validator": {},
    "attributes": {
      "element": "checkbox",
      "default": "True"
    }
  },
  "firstName": {
    "validator": {
      "required": true,
      "max": 30
    },
    "attributes": {
      "element": "text_field"
    }
  },
  "lastName": {
    "validator": {
      "required": true,
      "max": 150
    },
    "attributes": {
      "element": "text_field"

    }
  },
  "email": {
    "validator": {
      "required": true,
      "max": 254,
      "email": true
    },
    "attributes": {
      "element": "text_field",
      "type": "email"
    }
  },
  "role": {
    "validator": {
      "required": true,
      "max": 55,
      "oneOf": [
        "TEMP_WORKER",
        "CLIENT",
        "COMPANY_EMPLOYEE"
      ]
    },
    "attributes": {
      "element": "select",
      "options": [
        "Temp Worker",
        "Client",
        "Company employee"
      ]
    }
  },
  "dateOfBirth": {
    "attributes": {
      "element": "date"
    }
  },
  "maxAmountOfHours": {
    "validator": {},
    "attributes": {
      "element": "text_field",
      "type": "number"
    }
  },
  "profileImage": {
    "validator": {},
    "attributes": {
      "element": "textarea"
    }
  },
  "language": {
    "validator": {
      "max": 8,
      "oneOf": [
        "nl",
        "en"
      ]
    },
    "attributes": {
      "element": "select",
      "options": [
        "nl",
        "en"
      ],
      "default": "en"
    }
  }
}