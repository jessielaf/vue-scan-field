{
  "version": "0.2.3",
  "name": "vue-scan-field",
  "description": "Vue scan field: streamline your backend and frontend validation",
  "scripts": {
    "build:vuetify": "rollup --config rollup.config.js --framework vuetify",
    "build:quasar": "rollup --config rollup.config.js --framework quasar",
    "build": "npm run build:vuetify && npm run build:quasar",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve tests/main.js"
  },
  "dependencies": {
    "@vue/compat": "^3.2.19",
    "@vue/compiler-sfc": "^3.2.19",
    "core-js": "^3.11.1",
    "date-fns": "^2.15.0",
    "quasar": "2",
    "vee-validate": "^4.4.3",
    "vuex": "4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@vue/cli-service": "^4.5.13",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "rollup": "^2.50.6",
    "rollup-plugin-jscc": "^2.0.0",
    "rollup-plugin-pug": "^1.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue": "^3.1.0",
    "vuetify": "^3.0.0-alpha.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "main": "dist/vuetify.js",
  "browser": {
    "./sfc": "src/vue-scan-field.vue"
  },
  "files": [
    "dist/*",
    "src/*",
    "types/*",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
